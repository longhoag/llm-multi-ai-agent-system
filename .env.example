# Environment Configuration for LLM Multi-Agent System
# Copy this file to .env and fill in your actual values

# API Keys (Required)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# AWS Configuration (Required)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# S3 Bucket Names (Required)
S3_BUCKET_RAW_DATA=your-project-stock-data-raw
S3_BUCKET_PROCESSED=your-project-stock-data-processed
S3_BUCKET_MODELS=your-project-ml-models

# DynamoDB Table for Agent States
DYNAMODB_TABLE=agent-states

# System Configuration (Optional)
MAX_RETRIES=3
RETRY_DELAY=60
RATE_LIMIT_DELAY=12.0
LOG_LEVEL=INFO
MESSAGE_BUS_TIMEOUT=1.0

# Stock Symbols (Optional)
DEFAULT_SYMBOLS=AAPL,GOOGL,MSFT,TSLA,AMZN,NVDA,META

# LLM Configuration (Optional)
LLM_TEMPERATURE=0
LLM_MAX_TOKENS=1000

# Development Settings (Optional)
DEVELOPMENT_MODE=true
MOCK_APIS=false
